{
  "name": "gitlab-snippets",
  "displayName": "gitlab-snippets",
  "description": "Use gilab snippets easily",
  "version": "0.0.1",
  "publisher": "jyee721",
  "author": {
    "name": "Jennie Ji",
    "email": "jennie.ji@hotmail.com"
  },
  "license": "MIT",
  "engines": {
    "vscode": "^1.43.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onViewContainer:gitlab-snippets-explorer"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "gitlabSnippets.showStared",
        "category": "Gitlab snippets",
        "title": "Show stared"
      },
      {
        "command": "gitlabSnippets.publish",
        "category": "Gitlab snippets",
        "title": "Publish snippet"
      },
      {
        "command": "gitlabSnippets.addHost",
        "category": "Gitlab snippets",
        "title": "Add gitlab host"
      },
      {
        "command": "gitlabSnippets.reload",
        "title": "Reload snippets",
        "icon": "resources/icons/reload.svg"
      },
      {
        "command": "gitlabSnippets.star",
        "title": "Reload snippets",
        "icon": "resources/icons/star.svg"
      },
      {
        "command": "gitlabSnippets.unstar",
        "title": "Reload snippets",
        "icon": "resources/icons/star-filled.svg"
      }
    ],
    "menus": {
      "commandPalette": [
        {
          "command": "gitlabSnippets.showStared"
        },
        {
          "command": "gitlabSnippets.publish"
        },
        {
          "command": "gitlabSnippets.addHost"
        }
      ],
      "view/item/context": [
        {
          "command": "gitlabSnippets.download",
          "when": "view =~ /gitlabSnippets/ && viewItem == snippet",
          "group": "inline"
        },
        {
          "command": "gitlabSnippets.star",
          "when": "view == gitlabSnippetsExplorer-all && viewItem == snippet",
          "group": "inline"
        },
        {
          "command": "gitlabSnippets.unstar",
          "when": "view == gitlabSnippetsExplorer-stared && viewItem == snippet",
          "group": "inline"
        }
      ],
      "view/title": [
        {
          "command": "gitlabSnippets.reload",
          "when": "view =~ /gitlabSnippets/"
        }
      ]
    },
    "viewsContainers": {
      "activitybar": [
        {
          "id": "gitlabSnippetsExplorer",
          "title": "Gitlab Snippets Explorer",
          "icon": "resources/icons/gitlab-icon.svg"
        }
      ]
    },
    "views": {
      "gitlabSnippetsExplorer": [
        {
          "id": "gitlabSnippetsExplorer-stared",
          "name": "Stared"
        },
        {
          "id": "gitlabSnippetsExplorer-all",
          "name": "Accessable snippets"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "lint": "eslint src --ext ts",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile && yarn run lint",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/mocha": "^7.0.1",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.43.0",
    "@typescript-eslint/eslint-plugin": "^2.18.0",
    "@typescript-eslint/parser": "^2.18.0",
    "eslint": "^6.8.0",
    "glob": "^7.1.6",
    "mocha": "^7.0.1",
    "node-fetch": "^2.6.0",
    "typescript": "^3.7.5",
    "vscode-test": "^1.3.0"
  },
  "dependencies": {
    "@types/node-fetch": "^2.5.6"
  }
}
